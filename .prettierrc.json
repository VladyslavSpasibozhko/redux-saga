{
  "bracketSpacing": true,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "consistent",
  "endOfLine": "auto",
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
